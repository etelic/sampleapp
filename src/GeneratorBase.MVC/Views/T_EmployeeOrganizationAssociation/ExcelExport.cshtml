@model PagedList.IPagedList<GeneratorBase.MVC.Models.T_EmployeeOrganizationAssociation>
@using PagedList.Mvc;
@{
    ViewBag.Title = "Index";
    Layout = null;
    Response.ContentType = "application/vnd.ms-excel";
    Response.AddHeader("Content-Disposition", "attachment; filename=EmployeeOrganizationAssociation.xls");
}
                    <table>
                            <tr>
@if(User.CanView("T_EmployeeOrganizationAssociation","T_EmployeeID"))
{
		<th  @(Convert.ToString(ViewData["HostingEntity"]) == "T_Employee"?"hidden":"")>Employee</th>
}
@if(User.CanView("T_EmployeeOrganizationAssociation","T_OrganizationID"))
{
		<th  @(Convert.ToString(ViewData["HostingEntity"]) == "T_Organization"?"hidden":"")>Organization</th>
}
    </tr>
@foreach (var item in Model) {
    <tr>
	@if(User.CanView("T_EmployeeOrganizationAssociation","T_EmployeeID"))
{
		<td @(Convert.ToString(ViewData["HostingEntity"]) == "T_Employee"?"hidden":"")>
            @Html.DisplayFor(modelItem => item.t_employee.DisplayValue)
        </td>
}
@if(User.CanView("T_EmployeeOrganizationAssociation","T_OrganizationID"))
{
		<td @(Convert.ToString(ViewData["HostingEntity"]) == "T_Organization"?"hidden":"")>
            @Html.DisplayFor(modelItem => item.t_organization.DisplayValue)
        </td>
}
                                </tr>
                            }
</table>
               
