<div class='col-sm-6 col-md-6 col-xs-12' id="dvT_IsOpen">
	<div class='form-group' title="@Model.T_IsOpen">
		<label class="col-sm-5 col-md-5 col-xs-12">@Html.DisplayNameFor(model => model.T_IsOpen) </label>
		<div class="input-group col-sm-7 col-md-7 col-xs-12">
		
				@Html.DisplayFor(model => model.T_IsOpen)
		</div>
	</div>
</div>
}
						@if (User.CanView("T_Session", "ScheduleSessionID") && User.CanView("T_Schedule"))
						{
							<div class="col-md-12 col-sm-12 col-xs-12 hidden" id="dvScheduleSession">
								<div class="panel panel-default AppForm">
									<div class="panel-sub-heading clearfix">
										<h3 class="panel-title">@Html.LabelFor(model => model.ScheduleSessionID)</h3>
										</div>
										<div class="panel-body">
											<div class="row">
