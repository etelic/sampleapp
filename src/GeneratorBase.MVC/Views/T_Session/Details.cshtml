		<div class="input-group col-sm-7 col-md-7 col-xs-12">
		@if (@Model.T_SessionTimeSlotAssociatonID == 0 || @Model.T_SessionTimeSlotAssociatonID == null || @Model.t_sessiontimeslotassociaton == null)
		{
	         <p class="viewlabel"></p>
        }
        else
        {
		     <p class="viewlabel" title="@Model.t_sessiontimeslotassociaton.DisplayValue">@Model.t_sessiontimeslotassociaton.DisplayValue</p>
        }
		</div>
	</div>
</div>
}
@if(User.CanView("T_Session","T_IsOpen"))
