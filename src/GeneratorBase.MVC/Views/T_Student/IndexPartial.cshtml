		<p @(Convert.ToString(ViewData["HostingEntity"]) == "T_School" && Convert.ToString(ViewData["AssociatedType"]) == "T_SchoolName"?"hidden":"")>
		<span class="text-primary"> SchoolName </span> : <span>
		            @Html.DisplayFor(modelItem => item.t_schoolname.DisplayValue)
			         </span> </p>
		 }
@if(User.CanView("T_Student","T_DepartmentCodeID"))
{
		<p @(Convert.ToString(ViewData["HostingEntity"]) == "T_Department" && Convert.ToString(ViewData["AssociatedType"]) == "T_DepartmentCode"?"hidden":"")>
		<span class="text-primary"> DepartmentCode </span> : <span>
		            @Html.DisplayFor(modelItem => item.t_departmentcode.DisplayValue)
			         </span> </p>
		 }
@if(User.CanView("T_Student","T_Name"))
{
		<p><span class="text-primary"> Name </span> : <span>
            @Html.DisplayFor(modelItem => item.T_Name)
         </span> </p>	
		 }
@if(User.CanView("T_Student","T_Description"))
{
		<p><span class="text-primary"> Description </span> : <span>
            @Html.DisplayFor(modelItem => item.T_Description)
         </span> </p>	
		 }
<span class="label label-primary text-linkwhite"> 
@Html.ActionLink("View", "Details","T_Student", new { id = item.Id },null)
</span>
<span class="label label-warning text-linkwhite"> 
@if (User.CanEditItem("T_Student", item, User))
				 {
					@Html.ActionLink("Edit", "Edit","T_Student", new { id = item.Id, UrlReferrer = BackUrl, AssociatedType=ViewData["AssociatedType"], HostingEntityName = @Convert.ToString(ViewData["HostingEntity"]), HostingEntityID = @Convert.ToString(ViewData["HostingEntityID"]) },null)  
				 }
				 </span>
				<span class="label label-danger text-linkwhite"> 
				@if (User.CanDeleteItem("T_Student", item, User))
				 {
					@Html.ActionLink("Delete", "Delete", "T_Student", new { id = item.Id, UrlReferrer = BackUrl, AssociatedType=ViewData["AssociatedType"], HostingEntityName = @Convert.ToString(ViewData["HostingEntity"]), HostingEntityID = @Convert.ToString(ViewData["HostingEntityID"]) }, null)    
				 }
				 </span>
				   </li>
                    }
                </ul>
  			   @if (Model.Count > 0)
                {
                    <div id="pagination">
                        @Html.PagedListPager(Model, page => Url.Action(ActionNameUrl, "T_Student", getSortHtmlAttributes(null, true, page, false)))
                        <div class="fixPageSize">
                            Page Size :
                            @Html.DropDownList("PageSize", new SelectList(new Dictionary<string, int> { { "10", 10 }, { "20", 20 }, { "50", 50 }, { "100", 100 } }, "Key", "Value")
							, new { @id = "pagesizelistT_Student",
							 @onchange = @Html.Raw("pagesizelistChange(event,'T_Student','"
							  + @User.JavaScriptEncodedName + "')"),
							 @Url = Html.Raw(@Url.Action(ActionNameUrl, "T_Student",
							 getSortHtmlAttributes(ViewBag.CurrentSort, ViewBag.Pages == 1 ? false : true,
							 null, false), null)) })
							<span style="text-align:right;"> Total Count: @(Model.TotalItemCount)</span>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div> 
 </div>
    <div class="modal fade" id="ShowGraphT_Student" tabindex="-1" role="dialog" aria-labelledby="ShowGraphT_StudentLabel" aria-hidden="true">
        <div class="modal-dialog" style="width:80%">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title" id="ShowGraphT_StudentLabel">Dashboard Student</h4>
                </div>
				 <div  class="modal-body" >
						<div id="dvShowGraphT_Student" data-url="@Url.Action("ShowGraph", "T_Student")">
						</div>
						<div style="clear:both"></div>
                </div>
                
            </div>
        </div>
    </div>
</div>
