@model Dictionary<string, string>
@{
    Layout = null;

}
<script src="@Url.Content("~/Scripts/jquery-1.10.2.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Entity.js")" type="text/javascript"></script>
@Scripts.Render("~/bundles/common1")
@Scripts.Render("~/bundles/common2")
<div class="panel panel-default AppForm">
    <div class="panel-body">
        <br />
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="col-sm-10 col-md-10 col-xs-12">
                    <div class='form-group'>
                        @foreach (var item in Model)
                        {

                            <a data-original-title='@item.Value' data-placement="top" style="padding:3px 5px;" onclick=@Html.Raw("NavigateToUrl('" + Url.Action("Create", item.Key, new { UrlReferrer = Request.UrlReferrer, AssociatedType=ViewData["AssociatedType"], HostingEntityName = @Convert.ToString(ViewData["HostingEntity"]), HostingEntityID = @Convert.ToString(ViewData["HostingEntityID"]),viewtype = Convert.ToString(ViewBag.TemplatesName) }, null) + "');")>
                                <input type="radio" name="opinions" id="radio-1" /> Add @item.Value
                            </a>
                            <br />
                        }
                    </div>
                </div>
            </div>

        </div>
        <button id="CancelQuickAdd" type="button" class="btn btn-default btn-sm pull-right" data-dismiss="modal" aria-hidden="true">Cancel</button>
    </div>
</div>

@Scripts.Render("~/bundles/bootstrap")
@Scripts.Render("~/bundles/jqueryval")
@Scripts.Render("~/bundles/common2")
@Scripts.Render("~/bundles/common3")
