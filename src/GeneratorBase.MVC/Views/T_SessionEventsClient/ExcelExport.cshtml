@model PagedList.IPagedList<GeneratorBase.MVC.Models.T_SessionEventsClient>
@using PagedList.Mvc;
@{
    ViewBag.Title = "Index";
    Layout = null;
    Response.ContentType = "application/vnd.ms-excel";
    Response.AddHeader("Content-Disposition", "attachment; filename=Session EventsClient.xls");
}
                    <table>
                            <tr>
@if(User.CanView("T_SessionEventsClient","T_ClientID"))
{
		<th  @(Convert.ToString(ViewData["HostingEntity"]) == "T_Client"?"hidden":"")>Client</th>
}
@if(User.CanView("T_SessionEventsClient","T_SessionEventsID"))
{
		<th  @(Convert.ToString(ViewData["HostingEntity"]) == "T_SessionEvents"?"hidden":"")>Session Events</th>
}
    </tr>
@foreach (var item in Model) {
    <tr>
	@if(User.CanView("T_SessionEventsClient","T_ClientID"))
{
		<td @(Convert.ToString(ViewData["HostingEntity"]) == "T_Client"?"hidden":"")>
            @Html.DisplayFor(modelItem => item.t_client.DisplayValue)
        </td>
}
@if(User.CanView("T_SessionEventsClient","T_SessionEventsID"))
{
		<td @(Convert.ToString(ViewData["HostingEntity"]) == "T_SessionEvents"?"hidden":"")>
            @Html.DisplayFor(modelItem => item.t_sessionevents.DisplayValue)
        </td>
}
                                </tr>
                            }
</table>
               
