@model GeneratorBase.MVC.Models.T_School
@{
    ViewBag.Title = "Create School";
}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-title">
		@{
                var hostingValue = "";
                if (ViewBag.DisplayVal != null)
                {
                    hostingValue = " - " + ViewBag.DisplayVal;
                }
            }
            <i class="glyphicon glyphicon-save text-primary"></i> School  <i class="glyphicon glyphicon-chevron-right small"></i> <span> Add School @hostingValue</span>
			<button class="btn btn-xs  btn-default tip-top pull-right" type="button" data-original-title="List View" data-placement="top" onclick=@Html.Raw("NavigateToUrl('" + Url.Action("Index", "T_School") + "');") style="padding:4px 10px 7px 10px; height:25px !important; margin:2px 3px 0px 0px; text-align:left;">
						 List
			</button>
        </h1>
        <h2 class="text-primary">@Html.DisplayFor(model => model.DisplayValue)</h2>
    </div>
    <!-- /.col-lg-12 -->
</div>
@Html.Partial("~/Views/T_School/CreatePartial.cshtml", Model)
